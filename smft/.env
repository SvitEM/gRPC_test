# SMFT (Smart Manufacturing Test) Client Configuration
# Override these values as needed for your environment

# Target server
DF_HOST=localhost
DF_PORT=50051

# Load test parameters
N_RPS=2000
T_DURATION_SEC=300
T_WARMUP_SEC=10

# DF Server artificial delay (for latency calculation)
DF_DELAY_MS=5

# TLS Certificate paths (relative to container working directory)
TLS_CA=certs/ca.crt
TLS_CERT=certs/client.crt
TLS_KEY=certs/client.key